        <div class="max-w-7xl mx-auto">
            <div class="grid grid-cols-2 lg:grid-cols-4 gap-8 text-center">
                <div>
                    <div class="text-5xl font-bold mb-2">50K+</div>
                    <div class="text-gray-600">Proyectos activos</div>
                </div>
                <div>
                    <div class="text-5xl font-bold mb-2">2B+</div>
                    <div class="text-gray-600">Eventos mensuales</div>
                </div>
                <div>
                    <div class="text-5xl font-bold mb-2">99.9%</div>
                    <div class="text-gray-600">Tiempo activo</div>
                </div>
                <div>
                    <div class="text-5xl font-bold mb-2">&lt;50ms</div>
                    <div class="text-gray-600">Latencia promedio</div>
                </div>
            </div>
        </div>
    </section>

