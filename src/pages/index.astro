                                <div class="bg-white p-4 rounded-lg shadow-sm">
                                    <div class="h-2 bg-green-200 rounded mb-3 w-1/2"></div>
                                    <div class="h-8 bg-green-400 rounded"></div>
                                </div>
                            </div>
                            <div class="bg-white p-6 rounded-lg shadow-sm">
